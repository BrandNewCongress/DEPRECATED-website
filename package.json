  "devDependencies": {
    "babel-eslint": "^6.0.4",
    "babel-register": "^6.8.0",
    "eslint": "^2.10.1",
    "react-addons-test-utils": "^15.1.0",
    "sass-loader": "^3.2.0",
    "eslint-config-airbnb": "^9.0.1",
    "eslint-plugin-import": "^1.8.0",
    "eslint-plugin-jsx-a11y": "^1.2.0",
    "eslint-plugin-react": "^5.1.1",
    "foreman": "^1.4.1",
    "nodemon": "^1.9.2",
    "redux-devtools": "^3.3.1",
    "webpack-dev-server": "^1.14.1"
  },
  "scripts": {
    "clean": "rm -rf $OUTPUT_DIR",
    "lint": "eslint --fix --ext js --ext jsx src",
    "prod-build-client": "webpack --config ./webpack/config.js",
    "prod-build-server": "babel ./src -d $OUTPUT_DIR/server --source-maps && mkdir -p $OUTPUT_DIR/server/data && cp -R ./src/data/* $OUTPUT_DIR/server/data",
    "prod-build": "npm run clean && npm run prod-build-client && npm run prod-build-server",
    "postinstall": "if [ \"$NODE_ENV\" = production ] ; then npm run prod-build ; fi",
    "start": "node $OUTPUT_DIR/server/server",
    "predev-start": "npm run clean",
    "dev-start": "nodemon -e js,jsx -w ./src --exec ./tools/babel-run -- ./src/server",
    "dev": "nf start -w --procfile ./tools/Procfile.dev"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/BrandNewCongress/starter-kit.git"
  },
  "keywords": [
    "BrandNewCongress",
    "Brand",
    "New",
    "Congress",
    "StarterKit"
  ],
